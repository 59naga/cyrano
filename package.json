{
  "name": "cyrano",
  "version": "0.0.0-beta.1",
  "main": "lib",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "bluebird": "^2.10.2"
  },
  "scripts": {
    "start": "abigail ava PKG",
    "test": "abigail cover,lint",
    "cover": "nyc --reporter=lcov npm run ava",
    "ava": "ava --require babel-register",
    "lint": "eslint src test",
    "prepublish": "node -e \"if(process.env.TRAVIS_PASSED){}else{process.exit(1)}\" && npm run compile || echo skip prepublish",
    "compile": "babel -V && babel src --out-dir lib"
  },
  "ava": {
    "files": [
      "test/index.js"
    ]
  },
  "devDependencies": {
    "abigail": "^0.1.2",
    "ava": "^0.11.0",
    "babel-cli": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.4.3",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "nyc": "^5.5.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "keywords": [
    "game"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/59naga/cyrano.git"
  },
  "bugs": {
    "url": "https://github.com/59naga/cyrano/issues"
  },
  "homepage": "https://github.com/59naga/cyrano#readme",
  "author": "59naga <i59naga@icloud.com> (http://berabou.me)",
  "license": "MIT"
}
